<?xml version="1.0" encoding="utf-8"?>
<LWM2M  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://openmobilealliance.org/tech/profiles/LWM2M.xsd">
    <Object ObjectType="MODefinition">
        <Name>LightEffects</Name>
        <Description1>This IPSO object is dedicated to an effect visualizer. The output describe the effect usable or the effect current set in.</Description1>
        <ObjectID>20006</ObjectID>
        <ObjectURN>urn:oma:lwm2m:ext:20006</ObjectURN>
        <MultipleInstances>Single</MultipleInstances>
        <Mandatory>Optional</Mandatory>
        <Resources>
            <Item ID="10011">
                <Name>SupportedEffects</Name>
                <Operations>R</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Mandatory</Mandatory>
                <Type>String</Type>
                <RangeEnumeration></RangeEnumeration>
                <Units>%</Units>
                <Description>This resource represents a list of effect supported by the device.</Description>
            </Item>
            <Item ID="100012">
                <Name>CurrentEffect</Name>
                <Operations>RW</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Mandatory</Mandatory>
                <Type>String</Type>
                <RangeEnumeration></RangeEnumeration>
                <Units></Units>
                <Description>This resource represents the effect currently displayed.</Description>
            </Item>
        </Resources>
        <Description2></Description2>
    </Object>
</LWM2M>
