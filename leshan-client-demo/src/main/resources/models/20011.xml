<?xml version="1.0" encoding="utf-8"?>
<LWM2M  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://openmobilealliance.org/tech/profiles/LWM2M.xsd">
    <Object ObjectType="MODefinition">
        <Name>OpenClose</Name>
        <Description1>This IPSO object is dedicated to a remote door.</Description1>
        <ObjectID>20011</ObjectID>
        <ObjectURN>urn:oma:lwm2m:ext:20011</ObjectURN>
        <MultipleInstances>Single</MultipleInstances>
        <Mandatory>Optional</Mandatory>
        <Resources>
            <Item ID="10021">
                <Name>AvailableDirections</Name>
                <Operations>R</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Mandatory</Mandatory>
                <Type>String</Type>
                <RangeEnumeration></RangeEnumeration>
                <Units></Units>
                <Description>This resource describes the directions available.</Description>
            </Item>
            <Item ID="100022">
                <Name>Direction</Name>
                <Operations>RW</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Mandatory</Mandatory>
                <Type>String</Type>
                <RangeEnumeration></RangeEnumeration>
                <Units></Units>
                <Description>This resource represents the direction the door will open to.</Description>
            </Item>
            <Item ID="100023">
                <Name>openPercent</Name>
                <Operations>RW</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Mandatory</Mandatory>
                <Type>Integer</Type>
                <RangeEnumeration>0-100</RangeEnumeration>
                <Units>%</Units>
                <Description>This resource represents the percentage the door is open. </Description>
            </Item>
        </Resources>
        <Description2></Description2>
    </Object>
</LWM2M>
