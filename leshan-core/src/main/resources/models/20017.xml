<?xml version="1.0" encoding="utf-8"?>
<LWM2M  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://openmobilealliance.org/tech/profiles/LWM2M.xsd">
    <Object ObjectType="MODefinition">
        <Name>Timer</Name>
        <Description1>This IPSO object is dedicated to remote ac_unit. The output describe the range of speeds and the current speed set in.</Description1>
        <ObjectID>20017</ObjectID>
        <ObjectURN>urn:oma:lwm2m:ext:20017</ObjectURN>
        <MultipleInstances>Single</MultipleInstances>
        <Mandatory>Optional</Mandatory>
        <Resources>
            <Item ID="10041">
                <Name>MaxTime</Name>
                <Operations>R</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Mandatory</Mandatory>
                <Type>Integer</Type>
                <RangeEnumeration></RangeEnumeration>
                <Units></Units>
                <Description>This resource represents the speed of the Fan which has an Integer value between 0 and 100 as a percentage</Description>
            </Item>
            <Item ID="100042">
                <Name>TimeRemaing</Name>
                <Operations>RW</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Mandatory</Mandatory>
                <Type>Boolean</Type>
                <RangeEnumeration></RangeEnumeration>
                <Units></Units>
                <Description>This resource represents a verse of the fan. If the value is false the fan is in the default direction</Description>
            </Item>
            <Item ID="100030">
                <Name>isPaused</Name>
                <Operations>RW</Operations>
                <MultipleInstances>Single</MultipleInstances>
                <Mandatory>Mandatory</Mandatory>
                <Type>Boolean</Type>
                <RangeEnumeration></RangeEnumeration>
                <Units></Units>
                <Description>This resource define if the device is paused.</Description>
            </Item>
        </Resources>
        <Description2></Description2>
    </Object>
</LWM2M>
